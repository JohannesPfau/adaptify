<?xml version="1.0"?><FubiRecognizers globalMinConfidence="0.51" xmlns="http://www.hcm-lab.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.hcm-lab.de FubiRecognizers.xsd">  <!--ArmsCrossed-->  <JointRelationRecognizer name="RightHandDownInside" minConfidence="0.25">    <Joints main="rightWrist" relative="rightElbow"/>    <Relation type="above" max="90"/>    <Relation type="below" max="140"/>    <Relation type="leftOf" min="100"/>    <Relation type="inFrontOf" max="170"/>  </JointRelationRecognizer>  <JointRelationRecognizer name="LeftHandDownInside" minConfidence="0.25">    <Joints main="leftWrist" relative="leftElbow"/>    <Relation type="above" max="90"/>    <Relation type="below" max="140"/>    <Relation type="rightOf" min="100"/>    <Relation type="inFrontOf" max="170"/>  </JointRelationRecognizer>  <JointRelationRecognizer name="RightElbowDown">    <Joints main="rightElbow" relative="rightShoulder"/>    <Relation type="below" min="100"/>  </JointRelationRecognizer>  <JointRelationRecognizer name="LeftElbowDown">    <Joints main="leftElbow" relative="leftShoulder"/>    <Relation type="below" min="100"/>  </JointRelationRecognizer>  <CombinationRecognizer name="ArmsCrossedAFewSeconds">    <State minDuration="0.5" maxInterruptionTime="0.1">      <Recognizer name="RightHandDownInside" ignoreOnTrackingError="true"/>      <Recognizer name="LeftHandDownInside" ignoreOnTrackingError="true"/>      <Recognizer name="RightElbowDown" ignoreOnTrackingError="true"/>      <Recognizer name="LeftElbowDown" ignoreOnTrackingError="true"/>    </State>  </CombinationRecognizer>  <!--HandsFrontDown-->  <JointRelationRecognizer name="RightHandLow">    <Joints main="rightHand" relative="rightHip"/>    <Relation type="rightOf" max="150"/>    <Relation type="above" max="150"/>  </JointRelationRecognizer>  <JointRelationRecognizer name="LeftHandLow">    <Joints main="leftHand" relative="leftHip"/>    <Relation type="leftOf" max="150"/>    <Relation type="above" max="150"/>  </JointRelationRecognizer>  <JointRelationRecognizer name="HandsTogether">    <Joints main="rightHand" relative="leftHand"/>    <Relation type="apartOf" max="250"/>  </JointRelationRecognizer>  <CombinationRecognizer name="HandsFrontDown">    <State minDuration="1">      <Recognizer name="RightHandLow" minConfidence="0.2"/>      <Recognizer name="LeftHandLow" minConfidence="0.2"/>      <Recognizer name="HandsTogether" minConfidence="0.2"/>    </State>  </CombinationRecognizer>  <!-- Hands out-->  <JointRelationRecognizer name="RightHandRightOfShoulder" visibility="hidden">    <Joints main="rightWrist" relative="rightShoulder"/>    <Relation type="rightOf" min="250"/>  </JointRelationRecognizer>  <JointRelationRecognizer name="LeftHandLeftOfShoulder" visibility="hidden">    <Joints main="leftWrist" relative="leftShoulder"/>    <Relation type="leftOf" min="250"/>  </JointRelationRecognizer>  <CombinationRecognizer name="HandsOutAFewSeconds">    <State minDuration="1.5">      <Recognizer name="RightHandRightOfShoulder"/>      <Recognizer name="LeftHandLeftOfShoulder"/>    </State>  </CombinationRecognizer>   <!--HeadNod-->  <AngularMovementRecognizer name="HeadDown" useFilteredData="true">    <Joint name="head"/>    <BasicAngularVelocity type="pitchDown" min="15"/>  </AngularMovementRecognizer>  <AngularMovementRecognizer name="HeadUp" useFilteredData="true">    <Joint name="head"/>    <BasicAngularVelocity type="pitchUp" min="15"/>  </AngularMovementRecognizer>  <CombinationRecognizer name="HeadNod">    <State minDuration="0.05" maxDuration="0.8" timeForTransition="0.5">      <Recognizer name="HeadUp"/>    </State>    <State minDuration="0.05" maxDuration="0.8" timeForTransition="0.5">      <Recognizer name="HeadDown"/>    </State>    <State minDuration="0.05" maxDuration="0.8" timeForTransition="0.5">      <Recognizer name="HeadUp"/>    </State>    <State minDuration="0.05">      <Recognizer name="HeadDown"/>    </State>  </CombinationRecognizer>    <!--HeadShake-->  <AngularMovementRecognizer name="HeadLeft" useFilteredData="true">    <Joint name="head"/>    <BasicAngularVelocity type="yawLeft" min="20"/>  </AngularMovementRecognizer>  <AngularMovementRecognizer name="HeadRight" useFilteredData="true">    <Joint name="head"/>    <BasicAngularVelocity type="yawRight" min="20"/>  </AngularMovementRecognizer>  <CombinationRecognizer name="HeadShake">    <State minDuration="0.05" maxDuration="0.8" timeForTransition="0.5">      <Recognizer name="HeadLeft"/>    </State>    <State minDuration="0.05" maxDuration="0.8" timeForTransition="0.5">      <Recognizer name="HeadRight"/>    </State>    <State minDuration="0.05" maxDuration="0.8" timeForTransition="0.5">      <Recognizer name="HeadLeft"/>    </State>    <State minDuration="0.05" >      <Recognizer name="HeadRight"/>    </State>  </CombinationRecognizer>  <!--Pointing Forward (with right hand)-->  <LinearMovementRecognizer name="rightHandStill" visibility="hidden">    <Joints main="rightHand" relative="rightShoulder"/>    <Speed min="0" max="500"/>  </LinearMovementRecognizer>  <JointRelationRecognizer name="rightHandToUpperFront" visibility="hidden">    <Joints main="rightHand" relative="rightShoulder"/>    <MaxValues x="300"/>    <MinValues x="-350" y="-300"/>  </JointRelationRecognizer>  <CombinationRecognizer name="PointingForward">    <State minDuration="1.2">      <Recognizer name="PointingRight"/>      <Recognizer name="rightHandStill"/>      <Recognizer name="rightHandToUpperFront"/>    </State>  </CombinationRecognizer>  <!--Waving with right hand (above the shoulder)-->  <JointRelationRecognizer name="rightHandAboveShoulder" visibility="hidden">    <Joints main="rightHand" relative="rightShoulder"/>    <MinValues y="0"/>  </JointRelationRecognizer>  <LinearMovementRecognizer name="rightHandMovesRight" visibility="hidden">    <Joints main="rightHand" relative="rightShoulder"/>    <Direction x="1" y="0" z="0"/>    <Speed min="250"/>  </LinearMovementRecognizer>  <LinearMovementRecognizer name="rightHandMovesLeft" visibility="hidden">    <Joints main="rightHand" relative="rightShoulder"/>    <Direction x="-1" y="0" z="0"/>    <Speed min="250"/>  </LinearMovementRecognizer>  <CombinationRecognizer name="RightWaving">    <State minDuration="0.1" maxDuration="1.2" timeForTransition="0.3">      <Recognizer name="rightHandAboveTheShoulder" ignoreOnTrackingError="true"/>      <Recognizer name="rightHandMovesLeft"/>      <METAINFO>        <Property name="startSymbol" value="3"/>        <Property name="endSymbol" value="3"/>      </METAINFO>    </State>    <State minDuration="0.1" maxDuration="1.2" timeForTransition="0.3">      <Recognizer name="rightHandAboveTheShoulder" ignoreOnTrackingError="true"/>      <Recognizer name="rightHandMovesRight"/>      <METAINFO>        <Property name="startSymbol" value="1"/>        <Property name="endSymbol" value="1"/>      </METAINFO>    </State>    <State minDuration="0.1" maxDuration="1.2" timeForTransition="0.3">      <Recognizer name="rightHandAboveTheShoulder" ignoreOnTrackingError="true"/>      <Recognizer name="rightHandMovesLeft"/>      <METAINFO>        <Property name="startSymbol" value="3"/>        <Property name="endSymbol" value="3"/>      </METAINFO>    </State>    <State minDuration="0.1">      <Recognizer name="rightHandAboveTheShoulder" ignoreOnTrackingError="true"/>      <Recognizer name="rightHandMovesRight"/>      <METAINFO>        <Property name="startSymbol" value="1"/>        <Property name="endSymbol" value="1"/>      </METAINFO>    </State>  </CombinationRecognizer>  <!--Turn Away-->  <JointOrientationRecognizer name="TurnedLeft" visibility="hidden">    <Joint name="torso"/>    <MinDegrees y="25"/>  </JointOrientationRecognizer>  <JointOrientationRecognizer name="TurnedRight" visibility="hidden">    <Joint name="torso"/>    <MaxDegrees y="-35"/>  </JointOrientationRecognizer>  <CombinationRecognizer name="TurnAway">    <State minDuration="0.5">      <Recognizer name="TurnedLeft"/>      <AlternativeRecognizers>        <Recognizer name="TurnedRight"/>      </AlternativeRecognizers>    </State>  </CombinationRecognizer>    <!--Treasure Chest-->  <!--Using linear movement with length restriction-->  <LinearMovementRecognizer name="rightHandRightTC" visibility="hidden">    <Joints main="rightWrist"/>    <BasicDirection type="right"/>    <Length min="1" measuringUnit="upperArmLength"/>  </LinearMovementRecognizer>  <LinearMovementRecognizer name="rightHandDownTC" visibility="hidden">    <Joints main="rightWrist"/>    <BasicDirection type="down"/>    <Length min="0.5" measuringUnit="upperArmLength"/>  </LinearMovementRecognizer>  <LinearMovementRecognizer name="rightHandLeftTC" visibility="hidden">    <Joints main="rightWrist"/>    <BasicDirection type="left"/>    <Length min="1" measuringUnit="upperArmLength"/>  </LinearMovementRecognizer>  <LinearMovementRecognizer name="leftHandLeftTC" visibility="hidden">    <Joints main="leftWrist"/>    <BasicDirection type="left"/>    <Length min="1" measuringUnit="upperArmLength"/>  </LinearMovementRecognizer>  <LinearMovementRecognizer name="leftHandDownTC" visibility="hidden">    <Joints main="leftWrist"/>    <BasicDirection type="down"/>    <Length min="0.5" measuringUnit="upperArmLength"/>  </LinearMovementRecognizer>  <LinearMovementRecognizer name="leftHandRightTC" visibility="hidden">    <Joints main="leftWrist"/>    <BasicDirection type="right"/>    <Length min="1" measuringUnit="upperArmLength"/>  </LinearMovementRecognizer>  <CombinationRecognizer name="TreasureChest">    <State maxDuration="3" minDuration="0.2" maxInterruptionTime="0.2" timeForTransition="2">      <Recognizer name="rightHandRightTC"/>      <Recognizer name="leftHandLeftTC"/>      <METAINFO>        <Property name="startSymbol" value="0"/>        <Property name="endSymbol" value="0"/>      </METAINFO>    </State>    <State maxDuration="3" minDuration="0.15" maxInterruptionTime="0.2" timeForTransition="2">      <Recognizer name="rightHandDownTC"/>      <Recognizer name="leftHandDownTC"/>      <METAINFO>        <Property name="startSymbol" value="1"/>        <Property name="endSymbol" value="1"/>      </METAINFO>    </State>    <State minDuration="0.15" maxInterruptionTime="0.2">      <Recognizer name="rightHandLeftTC"/>      <Recognizer name="leftHandRightTC"/>      <METAINFO>        <Property name="startSymbol" value="2"/>        <Property name="endSymbol" value="2"/>      </METAINFO>    </State>  </CombinationRecognizer></FubiRecognizers>